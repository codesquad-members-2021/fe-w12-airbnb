                        <div class="header__main__search">
                            <div class="header__main__search__types">                            
                                <!-- label의 for는 label안에 요소가 있으면 안써도 됨!  -->
                                <input type="radio" name="selSearchType" id="rooms" checked/>
                                <label for="rooms">숙소</label>                            
                                
                                <input type="radio" name="selSearchType" id="experience"/>
                                <label for="experience">체험</label>
                                
                                <input type="radio" name="selSearchType" id="onlineExperience"/> 
                                <label for="onlineExperience">온라인 체험</label>
                            </div>
                            <div class="header__main__search__bar">      
                                <input type="hidden" name="prevSearchBarOption" id="prevSearchBarOption"/>

                                <input type="radio" name="selSearchBarOptions" id="locationInput"/>
                                <label for="locationInput" class="locationType">
                                    <p>위치</p>
                                    <p class="information">어디로 여행가세요?</p>
                                </label>
                                <input type="radio" name="selSearchBarOptions" id="checkin"/>
                                <label for="checkin" class="roomsType">
                                    <p>체크인</p>
                                    <p class="information" id="checkin">날짜 추가</p>
                                </label>
                                <input type="radio" name="selSearchBarOptions" id="checkout"/>
